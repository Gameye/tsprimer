{
  "name": "tsprimer",
  "version": "0.0.0",
  "description": "",
  "main": "./obj/main.js",
  "types": "./obj/main.d.ts",
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc",
    "test": "npm run lint && npm run spec-all",
    "docs": "typedoc --excludePrivate --excludeExternals --readme readme.markdown --out docs --theme default --name 'tsprimer'",
    "lint": "tslint 'src/**/*.ts'",
    "spec": "tape --require 'ts-node/register'",
    "spec-all": "npm run spec 'src/**/*.spec.ts'",
    "coverage": "nyc --report-dir report --reporter text-summary --reporter lcov --include 'src/**/*.ts' --exclude 'src/**/*.spec.ts' --extension '.ts' npm run spec-all"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "@types/tape": "^4.2.31",
    "blue-tape": "^1.0.0",
    "nyc": "^11.2.1",
    "tape": "^4.8.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "tslib": "^1.8.0"
  }
}
